Package: GeoFIS
Type: Package
Title: Spatial Data Processing for Decision Making
Version: 1.0.4
Authors@R: c(
  person("Serge", "Guillaume", email = "serge.guillaume@inrae.fr", role = "aut"),
  person("Jean-Luc", "Lablée", email = "jean-luc.lablee@inrae.fr", role = c("aut", "cre")),
